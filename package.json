{
  "name": "codementor",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "watch": "concurrently --names 'webpack, stylus' --prefix name 'npm run start' 'npm run styles:watch'",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "styles": "stylus -u autoprefixer-stylus ./src/css/style.styl -o ./src/css/style.css",
    "styles:watch": "stylus -u autoprefixer-stylus -w ./src/css/style.styl -o ./src/css/style.css",
    "deploy": "ns ./build --cmd 'list ./content -s'"
  },
  "devDependencies": {
    "react-scripts": "0.7.0",
    "autoprefixer-stylus": "0.10.0",
    "concurrently": "3.0.0",
    "stylus": "0.54.5"
  },
  "dependencies": {
    "history": "4.2.0",
    "re-base": "2.2.0",
    "react": "15.3.2",
    "react-addons-css-transition-group": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "^4.4.6",
    "react-router": "4.0.0-alpha.3",
    "redux": "^3.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebD00D/codementor.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WebD00D/codementor/issues"
  },
  "homepage": "https://github.com/WebD00D/codementor#readme"
}
